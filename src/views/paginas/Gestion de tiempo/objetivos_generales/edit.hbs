<h2 class="titulo_principal">Editar Objetivos <b class="color_verde">{{Datos.objetivo}}</b></h2>

<form action="/objetivos/edit/{{Datos.id}}" method="POST" class="contenedor_form">
	<div class="contenedor_inputs">
		<input type="TEXT" name="objetivo_name" placeholder="Nombre de objetivo" value="{{Datos.objetivo}}" required class="color_blanco">
		<input type="number" name="objetivo_posicion" placeholder="posicion" value="{{Datos.posicion}}" required class="color_naranja">
	</div>
	<input type="submit" value="Enviar" class="btn_enviar">
</form>